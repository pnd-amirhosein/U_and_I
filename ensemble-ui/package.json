{
  "name": "ensemble-ui",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "types": "dist/types/components.d.ts",
  "main": "dist/index.cjs.js",
  "module": "dist/index.mjs",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/ensemble-ui/ensemble-ui.esm.js",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/ensemble-ui/ensemble-ui.esm.js",
      "require": "./dist/ensemble-ui/ensemble-ui.cjs.js"
    },
    "./dist/components/*": {
      "import": "./dist/components/*",
      "types": "./dist/components/*"
    },
    "./my-component": {
      "import": "./dist/components/my-component.js",
      "types": "./dist/components/my-component.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stenciljs/component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "npm run build:all && stencil build",
    "build:all": "npm run delete:old && npm run clean:build && npm run generate:font && npm run generate:tokens && npm run generate:dist-tokens && npm run generate:icon",
    "tokens": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' packages/core/tokens/generate-tokens.ts",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "generate:font": "node packages/core/tokens/generate-fonts.js",
    "generate:tokens": "npx tsc packages/core/tokens/generate-tokens.ts --outDir dist-tokens",
    "generate:dist-tokens": "node dist-tokens/generate-tokens.js",
    "generate:icon": "node scripts/icon-generator.mjs",
    "clean": "rimraf dist dist-tokens node_modules/.stencil .stencil",
    "clean:build": "node scripts/clean-output.js",
    "delete:old": "node scripts/delete-old.js",
    "dev:smart": "node scripts/dev-orchestrator.js"
  },
  "devDependencies": {
    "@angular/compiler": "^20.3.0",
    "@angular/compiler-cli": "^20.3.0",
    "@stencil/angular-output-target": "^1.1.1",
    "@stencil/core": "^4.37.0",
    "@stencil/postcss": "^2.1.0",
    "@stencil/react-output-target": "^1.2.0",
    "@stencil/sass": "^3.2.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.18.8",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "autoprefixer": "^10.4.21",
    "chalk": "^4.1.2",
    "cssnano": "^7.1.1",
    "fs-extra": "^11.3.2",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "postcss": "^8.5.6",
    "postcss-nesting": "^13.0.2",
    "postcss-preset-env": "^10.4.0",
    "puppeteer": "^24.3.0",
    "sass": "^1.93.2",
    "svgo": "^4.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "ensemble-icons": "file:../ensemble-icons"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "^20.3.0",
    "@angular/core": "^20.3.0",
    "fs": "^0.0.1-security"
  }
}